{
    "name": "graphql-apollo-mysql-typeorm",
    "version": "1.0.0",
    "description": "GraphQL API with Apollo Server, MySQL, TypeScript, TypeORM, Express, JWT, Subscriptions",
    "main": "./dist/app.js",
    "author": "AutanaSoft",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AutanaSoft/graphql-apollo-mysql-typeorm.git"
    },
    "homepage": "https://github.com/AutanaSoft/graphql-apollo-mysql-typeorm#readme",
    "bugs": {
        "url": "https://github.com/AutanaSoft/graphql-apollo-mysql-typeorm.git"
    },
    "scripts": {
        "dev": "nodemon ./src/app.ts",
        "build": "tsc --project tsconfig.prod.json -P .",
        "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/app.js",
        "start:dos": "node -r tsconfig-paths/register dist/app.js",
        "clean": "rm -rf dist/**/*.js dist/**/*.map",
        "clean:all": "rm -r dist/**",
        "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
    },
    "dependencies": {
        "@apollo/server": "^4.7.0",
        "@graphql-tools/schema": "^9.0.19",
        "bcryptjs": "^2.4.3",
        "body-parse": "^0.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "graphql": "^16.6.0",
        "graphql-subscriptions": "^2.0.0",
        "graphql-tag": "^2.12.6",
        "graphql-ws": "^5.12.1",
        "jsonwebtoken": "^9.0.0",
        "mysql2": "^3.2.4",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.15",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.13",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/node": "^18.16.2",
        "@types/ws": "^8.5.4",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "eslint": "^8.39.0",
        "eslint-config-prettier": "^8.8.0",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.8",
        "tsconfig-paths": "^4.2.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "keywords": [
        "nodejs",
        "typescript",
        "graphql",
        "apollo",
        "mysql",
        "typeorm",
        "express",
        "auth",
        "jwt",
        "subscription",
        "ws"
    ]
}
